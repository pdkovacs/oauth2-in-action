{
  "name": "noip-updater",
  "version": "1.0.0",
  "description": "Regularly check public IP and set the new IP on NOIP.com if changed",
  "main": "index.js",
  "dependencies": {
    "base64url": "^1.0.4",
    "body-parser": "^1.13.2",
    "consolidate": "^0.13.1",
    "cors": "^2.7.1",
    "express": "^4.13.1",
    "immutable": "^3.8.1",
    "jsrsasign": "^5.0.0",
    "nosql": "^3.0.3",
    "qs": "^4.0.0",
    "randomstring": "^1.0.7",
    "request": "^2.58.0",
    "sync-request": "^2.0.1",
    "underscore": "^1.8.3",
    "underscore.string": "^3.1.1",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "@types/base64url": "^2.0.0",
    "@types/consolidate": "0.0.33",
    "@types/express": "^4.0.39",
    "@types/node": "^7.0.12",
    "@types/qs": "^6.5.1",
    "@types/randomstring": "^1.1.6",
    "@types/underscore": "^1.8.5",
    "@types/underscore.string": "0.0.32",
    "@types/winston": "^2.3.0",
    "tslint": "^5.6.0",
    "typescript": "^2.4.2"
  },
  "scripts": {
    "lint": "tslint -c tslint.json '{src,test}/**/*.{ts,tsx}'",
    "start": "tsc && node build/index.js",
    "dev": "tsc --watch & cd build/ && nodemon ./index.js",
    "build": "tsc",
    "run": "rm -rf build dist && tsc && node dist/noip-updater.js",
    "package": "npm run build && tar -C $(dirname $PWD) -czf dist/$(basename $PWD).tgz --exclude 'node_modules/@types' --exclude '.DS_Store' $(basename $PWD)/node_modules $(basename $PWD)/build"
  },
  "author": "peter.dunay.kovacs@gmail.com",
  "license": "Apache-2.0"
}
